@{
    var books = ViewData["Books"] as firstapp.Models.BookModel;
    var allbooks = ViewData["AllBooks"] as IEnumerable<firstapp.Models.BookModel>;


}

<div class="jumbotron">
  <div class="container text-center"> 
      <h4>@ViewData["Title"]</h4>  
    <img src="\images\photo-1507842217343-583bb7270b66.jpg" class="img-responsive" style="width:100%;height:70px" alt="Image">
  </div>
</div>
  
<div class="container-fluid bg-3 text-center">    
  <h3>Some of my Books </h3><br>
  <div class="row">
    @foreach( var allbook in allbooks){
    <div class="col-sm-3">
      <p>Book: @allbook.Title</p>
      <img src="@allbook.Image" class="img-responsive" style="width:100%" alt="Image">
      <br>
      <p>Author: @allbook.Author</p>
    </div>
    }
  </div>
</div><br>


